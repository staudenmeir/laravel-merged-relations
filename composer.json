{
    "name": "staudenmeir/laravel-merged-relations",
    "description": "Merged Laravel Eloquent relationships",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^12.0",
        "staudenmeir/laravel-migration-views": "^1.11"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "laravel/framework": "^12.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench-core": "^10.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0",
        "staudenmeir/eloquent-has-many-deep": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "Staudenmeir\\LaravelMergedRelations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Staudenmeir\\LaravelMergedRelations\\DatabaseServiceProvider",
                "Staudenmeir\\LaravelMergedRelations\\IdeHelperServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
